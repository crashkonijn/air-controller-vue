{
  "name": "air-controller-vue",
  "description": "controller for AirController",
  "main": "src/index.js",
  "version": "1.2.1",
  "author": "Peter Klooster",
  "repository": {
    "type": "git",
    "url": "https://github.com/crashkonijn/air-controller-vue"
  },
  "keywords": [
    "air-controller-vue",
    "air-controller",
    "AirController",
    "AirConsole"
  ],
  "license": "MIT",
  "scripts": {
